{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/process-queue.ts": {
      "maxDuration": 300
    },
    "api/extract-hybrid.ts": {
      "maxDuration": 300
    }
  },
  "crons": [
    {
      "path": "/api/cron/keep-alive",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/backup-master-excel",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/backup-database",
      "schedule": "0 2 * * *"
    }
  ]
}
